
public class javaMain {

	public static void main(String[] args) {
		int nA = 0;
		System.out.printf("忙式式式式式式式式式成式式式式式成式式式式式式忖\n");
		System.out.printf("弛 Decimal 弛 Oct 弛 Haxa 弛\n");
		System.out.printf("戍式式式式式式式式式托式式式式式托式式式式式式扣\n");
		System.out.printf("弛  檜營錳   弛  1  弛 Haxa 弛\n");
		System.out.printf("戍式式式式式式式式式托式式式式式托式式式式式式扣\n");
		for(int i = 0; i < 16; i++) {
			
			System.out.printf("弛   %3d   弛 %2d 	弛   %#x  弛\n", nA,nA,nA);	
			nA++;
		}
		System.out.println("戌式式式式式式式式式扛式式式式式扛式式式式式式戎");
	}

}
