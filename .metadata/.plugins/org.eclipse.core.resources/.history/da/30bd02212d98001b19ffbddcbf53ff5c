package sdh.hs.kr;

public class HelloWorld {

	public static void main(String[] args) {
		//자동완성기능 - ctrl+space
		//문자출력
		System.out.println("HelloWorld");
		System.out.println("leejeawon");
		
		System.out.println(2021);//숫자
		System.out.println("2021");//문자
		System.out.println(3+4);//더하기
		System.out.println("3"+"4");//연결하기
		System.out.println(52-"33");
	}

}
