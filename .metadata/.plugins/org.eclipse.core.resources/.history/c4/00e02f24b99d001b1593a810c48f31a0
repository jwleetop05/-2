import java.util.Scanner;

public class java8 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int A = sc.nextInt();
		System.out.println("ют╥б : ");
		System.out.println(A + "seconds is equivalent to" + A/3600 + " "+ "hours.\n" + (A%3600)/60 + " minutes. "  );
	}

}
