
public class javamain {

	public static void main(String[] args) {
		int nA = 100;
		float fA = 0.654321f;
		System.out.printf("nA = [%d] \t fA = %f\n", nA, fA);
		System.out.printf("nA = [%1d] \t fA = %1.1f\n", nA, fA);
		System.out.printf("nA = [%2d] \t fA = %1.2f\n", nA, fA);
		System.out.printf("nA = [%3d] \t fA = %1.3f\n", nA, fA);
		System.out.printf("nA = [%4d] \t fA = %1.4f\n", nA, fA);
		System.out.printf("nA = [%5d] \t fA = %1.5f\n", nA, fA);
		System.out.printf("nA = [%6d] \t fA = %1.6f\n", nA, fA);

	}

}
