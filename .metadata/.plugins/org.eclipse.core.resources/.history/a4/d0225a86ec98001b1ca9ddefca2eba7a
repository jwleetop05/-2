package sdh.hs.kr;

import java.util.Scanner;

public class HellowWorld2 {

	public static void main(String[] args) {
		        Scanner sc = new Scanner(System.in);
		        int A = sc.nextInt();
		        int B = sc.nextInt();
		        sc.close();
		        
		        if(B<45) {
		               if(A == 0){
		                   A = 23;
		            	   System.out.printf("23 %d", (B+15));
		               }
		            System.out.printf("%d %d", (A-1), (B+15));
		            } 
		        
		       
		        
		    }
		
}

